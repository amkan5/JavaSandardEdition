package Step4;

public class MatrixStudy3 {
	public static void main(String[] args) {
		int[][] mtx = new int[5][5];
		int k=0;
		for (int i=0;i<5;i++) {
			if(i%2==0) {
			for (int j=1;j<5;j++) {
				k++;
				mtx[i][j] = k;
			}
			}else{
			for (j=)
			}
		}
		for (int i=0;i<5;i++) {
			for (j=0;j<5;j++) {
				System.out.print(mtx[i][j] + " " );
			}
			System.out.println("");
		}


	}

}
