package Step5;

public class Table {
	public static void main(String[] args) {
		int[][] mtx = new int[10][10];
		for (int i=1;i<10;i++) {
			for(int j=1;j<10;j++) {
				mtx[j][i] = (j)*(i);
			}
		}
		for (int i=1;i<10;i++) {
			for(int j=2;j<6;j++) {
				System.out.print(j+"x"+i+"="+mtx[i][j] + "\t");
			}
			System.out.println();
		for(int j=7;j<10;j++) {
			System.out.print(j+"x"+i+"="+mtx[i][j] + "\t");
		}

	}
	}
}
