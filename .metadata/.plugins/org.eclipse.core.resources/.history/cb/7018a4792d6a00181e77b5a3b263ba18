package Step5;

public class Table {
	public static void main(String[] args) {
		int[][] mtx = new int[20][20];
		int result = 0;
		for (int i=1;i<10;i++) {
			for(int j=2;j<10;j++) {
				mtx[j][i] = (j)*(i);
			}
		}
		for (int i=1;i<10;i++) {
			for(int j=2;j<10;j++) {
				System.out.print(mtx[i][j] + " ");
			}
			System.out.println();
		}

	}
}
